
pipeline {
	agent any

	stages {
		stage('code Checkout') {
			steps {
				git branch: 'main', credentialsId: 'github_cred', url: 'https://github.com/Priyankabavge12/jenkins.git'
			}
		}

		stage('Build') {
			steps {
				sh 'ls -lrt'
			}
		}

		stage('Test') {
			steps {
				sh """
					#!/bin/bash
					echo This is Integration testing
					sleep 5
					ls -lrt
				"""
			}
		}
	}
}
