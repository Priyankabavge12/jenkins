pipeline{
    agent any
    stages{
        stage('Code CHECKOUT'){
            steps{
                git branch: 'main', credentialsId: 'github_cred', url: 'https://github.com/Priyankabavge12/jenkins.git'
            }
        }
        stage('Build'){
            steps{
                sh 'ls -lrt'         }
        }
    }
}